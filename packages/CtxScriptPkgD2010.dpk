  rtl,
  dbrtl,
  vclactnband,
  vclx,
  vcldb;


contains
  CtxScript in 'CtxScript.pas',
  CtxScriptReg in 'CtxScriptReg.pas',
  CtxActions in 'CtxActions.pas',
  CtxPkgSysUtils in 'CtxPkgSysUtils.pas',
  CtxPkgClasses in 'CtxPkgClasses.pas',
  CtxUnitEditor in 'CtxUnitEditor.pas' {frmCtxUnitEditor},
  CtxPasCompiler in 'CtxPasCompiler.pas',
  CtxPkgDB in 'CtxPkgDB.pas',
  CtxUnit in 'CtxUnit.pas',
  CtxTextReport in 'CtxTextReport.pas',
  CtxPgkGraphics in 'CtxPgkGraphics.pas';

end.
